{
  "name": "signal-quality-ai-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "description": "AI-powered system that analyzes and scores the quality of buyer intent signals",
  "scripts": {
    "dev": "turbo run dev",
    "dev:backend": "turbo run dev --filter=backend",
    "dev:frontend": "turbo run dev --filter=frontend",
    "build": "turbo run build",
    "build:backend": "turbo run build --filter=backend",
    "build:frontend": "turbo run build --filter=frontend",
    "start:backend": "pnpm --filter backend start",
    "demo": "pnpm --filter backend demo",
    "test": "turbo run test",
    "lint": "turbo run lint"
  },
  "keywords": [
    "sales",
    "ai",
    "signals",
    "lead-scoring",
    "b2b"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "turbo": "^2.7.3"
  }
}
